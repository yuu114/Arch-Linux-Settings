XTerm*termName: xterm-256color
XTerm*foreground: white
XTerm*background: black
XTerm*locale: true

!-------------------------
! URXVT Terminal config
! --------------------------
URxvt*buffered: true
URxvt*font: xft:DejaVu Sans Mono :size=12:antialias=true
URxvt.termName: rxvt-256color
URxvt.letterSpace: -1

! transparency
!
! URxvt.transparent: true
! URxvt.shading: 40
URxvt.depth: 32

URxvt.scrollBar: false
URxvt.urlLauncher: firefox
URxvt.foreground: #FFF
URxvt.background: [60]#101000
URxvt.utf8: true
URxvt.locale: true


URxvt.cursorColor: yellow
URxvt.colorUL: red

URxvt.color0: #757575
URxvt.color1: #f44336
URxvt.color2: #4CAF50
URxvt.color3: #FFEB3B

XTerm*termName: xterm-256color
XTerm*foreground: white
XTerm*background: black
XTerm*locale: true

!-------------------------
! URXVT Terminal config
! --------------------------
URxvt*buffered: true
URxvt*font: xft:DejaVu Sans Mono :size=12:antialias=true
URxvt.termName: rxvt-256color
URxvt.letterSpace: -1

! transparency
!
! URxvt.transparent: true
! URxvt.shading: 40
URxvt.depth: 32

URxvt.scrollBar: false
URxvt.urlLauncher: firefox
URxvt.foreground: #FFF
URxvt.background: [60]#101000
URxvt.utf8: true
URxvt.locale: true


URxvt.cursorColor: yellow
URxvt.colorUL: red

!black
*color0:     #393939
*color8:     #AF875F
!red
*color1:     #BF1E2D
*color9:     #E7212A
!green
*color2:     #9DBA3A
*color10:    #A9C938
!yellow
*color3:     #F5B915
*color11:    #F7D325
!blue
*color4:     #1CA1DB
*color12:    #00AFDA
!magenta
*color5:     #652F90
*color13:    #894E9F
!cyan
*color6:     #EA7D24
*color14:    #F79321
!white
*color7:     #E7E8E9
*color15:    #F3F3F4
